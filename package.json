{
  "devDependencies": {
    "@dmsi/prettier-config": "^1.3.0",
    "@types/cuid": "^1.3.0",
    "del-cli": "^3.0.0",
    "husky": "^4.0.0",
    "lint-staged": "^9.5.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "name": "dunsany",
  "version": "0.0.0",
  "description": "A procedural pantheon generator.",
  "repository": "https://github.com/Renddslow/dunsaney",
  "author": "Matt McElwee <mubatt@wyopub.com>",
  "license": "CC-BY-NC-SA-2.0",
  "prettier": "@dmsi/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prebuild": "del-cli dist compile",
    "build": "tsc && microbundle",
    "postbuild": "del-cli compile",
    "prettier": "prettier *.{js,css,json,md,ts,tsx} --write"
  },
  "types": "dist",
  "dependencies": {
    "cuid": "^2.1.8"
  },
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "source": "src/index.js"
}
